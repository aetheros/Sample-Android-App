<?xml version="1.0" encoding="utf-8"?>
<!-- Aetheros splash screen -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <color android:color="@color/splash_screen_background_color"></color>
    </item>
    <item
        android:width="58dp"
        android:height="48dp"
        android:gravity="center">
        <bitmap
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:mipMap="true"
            android:src="@mipmap/aetheros_logo_no_text">
        </bitmap>
    </item>
</layer-list>
